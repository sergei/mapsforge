<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:installLocation="auto"
	package="org.mapsforge.applications.android.samples" android:versionCode="7" android:versionName="0.3.0">

	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.INTERNET" />

	<uses-sdk android:minSdkVersion="4" android:targetSdkVersion="14" />

	<application android:label="Samples">
		<activity android:name=".Samples" android:label="Samples">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".BasicMapViewer" android:label="BasicMapViewer" />
		<activity android:name=".DualMapViewer" android:label="DualMapViewer" />
		<activity android:name=".OverlayMapViewer" android:label="OverlayMapViewer" />
		<activity android:name=".DownloadMapViewer" android:label="DownloadMapViewer" />
	</application>
</manifest>